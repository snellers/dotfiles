
alias v='ls -l'
alias gd='git diff'
alias gdc='git diff --cached'
alias ga='git add'
alias gu='git pull'
alias gp='git push'
alias gc='git commit'
alias gri='git rebase -i'
alias ressh='eval `ssh-agent` ssh-add ~/.ssh/id_rsa'
alias cryptclose='sudo vgchange -an vgext0 && sudo cryptsetup luksClose /dev/mapper/luks*'
alias rsyncit="rsync -lruv --del "
export EDITOR=nvim
export ASAN_OPTIONS=detect_stack_use_after_return=true
export GPG_TTY=$(tty)

